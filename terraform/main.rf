terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 5.0"
    }
    random = {
      source  = "hashicorp/random"
      version = "~> 3.5"
    }
  }
}

# Configura o provedor da AWS para usar a região definida nas variáveis
provider "aws" {
  region = var.aws_region
}